{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "gen": "npx prisma generate",
    "push": "npx prisma db push",
    "pull": "npx prisma db pull",
    "nuke": "psql -Atx postgresql://xxxx.xxxxxx@localhost:5432/xxxxxx -f nuke.sql",
    "reset": "yarn nuke && prisma migrate dev -n reset",
    "connect": "ssh -i 'xxxx' xxxx",
    "cron-connect": "ssh -i 'xxxxx' xxxxxx.compute.amazonaws.com",
    "lint": "eslint .",
    "deploy":"yarn install && yarn gen && yarn push && yarn build"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@types/yargs": "^17.0.10",
    "aws-sdk": "^2.1092.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "d3-node": "^2.2.3",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "fs": "^0.0.1-security",
    "got": "^12.3.0",
    "http": "^0.0.1-security",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "node-fetch": "^3.2.1",
    "node-mailjet": "^3.3.7",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "stream-chat": "^6.6.0",
    "stripe": "^9.16.0",
    "twilio": "^3.78.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
